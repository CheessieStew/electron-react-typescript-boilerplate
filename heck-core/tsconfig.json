{

    "lib": ["dom", "es5", "es6", "es7", "dom.iterable", "scripthost"],
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "module": "esnext",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "compilerOptions": {
    "declaration": true,
    "outDir": "../bin/heck-core",
    "rootDir": ".",
    "composite": true,
    "strict": true
  },
  "include": ["src", "*.ts"]
}
